{
  "processes": [
    {
      "id": 1,
      "title": "新员工入职流程",
      "category": "hr",
      "subcategory": "招聘入职",
      "description": "规范新员工入职流程，确保顺利融入公司",
      "department": "人力资源部",
      "visibleTo": ["admin", "manager", "hr", "employee"],
      "steps": [
        {
          "number": 1,
          "title": "入职前准备",
          "description": "人力资源部准备办公设备、系统账号、门禁卡、工牌等"
        },
        {
          "number": 2,
          "title": "入职报到",
          "description": "新员工到人力资源部报到，提交身份证、学历证明、离职证明等材料"
        },
        {
          "number": 3,
          "title": "入职培训",
          "description": "参加公司规章制度、企业文化、安全培训等"
        },
        {
          "number": 4,
          "title": "部门报到",
          "description": "到分配部门报到，认识同事和领导，了解部门工作"
        },
        {
          "number": 5,
          "title": "试用期管理",
          "description": "试用期期间的工作安排、培训和考核"
        }
      ],
      "owner": "人力资源部",
      "version": "2.1",
      "createdAt": "2024-01-15",
      "updatedAt": "2024-03-10"
    },
    {
      "id": 2,
      "title": "费用报销流程",
      "category": "finance",
      "subcategory": "日常报销",
      "description": "规范员工费用报销流程，确保合规高效",
      "department": "财务部",
      "visibleTo": ["admin", "manager", "finance", "employee"],
      "steps": [
        {
          "number": 1,
          "title": "填写报销单",
          "description": "在财务系统中填写报销申请，附上发票和相关说明"
        },
        {
          "number": 2,
          "title": "部门审批",
          "description": "部门经理审批报销申请的合理性和必要性"
        },
        {
          "number": 3,
          "title": "财务审核",
          "description": "财务人员审核票据合规性、金额准确性和预算情况"
        },
        {
          "number": 4,
          "title": "付款处理",
          "description": "财务出纳根据审批结果进行付款处理"
        },
        {
          "number": 5,
          "title": "凭证归档",
          "description": "财务人员将相关凭证归档保存，完成报销流程"
        }
      ],
      "owner": "财务部",
      "version": "3.0",
      "createdAt": "2024-01-10",
      "updatedAt": "2024-02-28"
    },
    {
      "id": 3,
      "title": "软件开发流程",
      "category": "tech",
      "subcategory": "研发管理",
      "description": "软件项目从需求到上线的完整开发流程",
      "department": "技术部",
      "visibleTo": ["admin", "manager", "tech"],
      "steps": [
        {
